 <% if @usersJs %>
 <% @usersJs.each do |user| %>
<form class="form-horizontal">
          <fieldset>
          <h4>Сотрудник</h4><br>
          <div class="control-group">
              <label class="control-label">Должность</label>
              <div class="controls">
               <input name="fname" class="span2 uneditable-input" value="<%= user.doljnost %>" type="text">
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">Пол</label>
              <div class="controls">
               <input name="fname" class="span2 uneditable-input" value="<%= user.sex %>" type="text">
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">Дата рождения</label>
              <div class="controls">
               <input name="fname" class="span2 uneditable-input" value="<%= user.datebirth %>" type="text">
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">Дата приема</label>
              <div class="controls">
               <input name="fname" class="span2 uneditable-input" value="<%= user.datework %>" type="text">
              </div>
            </div>
          </fieldset>

         <fieldset>
          <h4>Контакты</h4><br>
            <div class="control-group">
              <label class="control-label">Телефон</label>
              <div class="controls">
               <input name="fname" class="span2 uneditable-input" value="<%= user.phone %>" type="text">
              </div>
            </div>
          </fieldset>

          <fieldset>
          <h4>Паспортные данные</h4><br>
            <div class="control-group">
              <label class="control-label">Серия и номер</label>
              <div class="controls">
               <input name="fname" class="span1 uneditable-input" value="<%= user.seriy %>" type="text">
              <input name="fname" class="span1 uneditable-input" value="<%= user.nomer %>" type="text">
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">Кем выдан</label>
              <div class="controls">
               <input name="fname" class="span3 uneditable-input" value="<%= user.vidan %>" type="text">
              </div>
            </div>
            
          </fieldset>
</form>
<% end %>

<% else %>

<%  end %>